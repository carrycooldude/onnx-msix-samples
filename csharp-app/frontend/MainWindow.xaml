
<!-- C# WinUI 3 Frontend MainWindow (Demo) -->
<Window
    x:Class="Frontend.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="ONNX Sample Frontend" Height="350" Width="525">
    <Grid>
        <Button x:Name="RunButton" Content="Run Inference" Width="120" Height="40" VerticalAlignment="Center" HorizontalAlignment="Center" Click="RunButton_Click"/>
        <TextBlock x:Name="ResultText" VerticalAlignment="Bottom" HorizontalAlignment="Center" Margin="0,0,0,30" FontSize="16"/>
    </Grid>
</Window>
